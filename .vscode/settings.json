{
    "cSpell.enabled": true,
    "cSpell.language": "en",
    "cSpell.words": [
        "devcontainer",
        "devcontainers",
        "textlint",
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        ".devcontainer",
    ],
    "editor.formatOnSave": true,
    "textlint.autoFixOnSave": true,
    "textlint.languages": [
        "markdown",
        "plaintext",
        "html",
        "tex",
        "latex",
        "doctex"
    ]
}
